suncorp-a = whitesmoke
suncorp-b = #006c64
suncorp-c = #62a6a4

#suncorp

  background suncorp-a
  color suncorp-b

  .btn
    background suncorp-c
    color suncorp-a

  .flex-control-paging li a
    border 1px suncorp-b solid

    &.flex-active
      background suncorp-b

  .flex-direction-nav a
    color darken(suncorp-a, 20%)

    &:hover
      color suncorp-b

